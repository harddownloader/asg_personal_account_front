{
  "trailingComma": "none",
  "useTabs": false,
  "printWidth": 100,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "newline-before-return": true,
  "no-duplicate-variable": [true, "check-parameters"]
}
